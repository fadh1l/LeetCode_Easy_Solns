class Solution:
    def canPlaceFlowers(self, flowerbed: List[int], n: int) -> bool:
        #pad the flowerbed ends
        f = [0] + flowerbed + [0]
        c = 0
        for i in range(1, len(f)-1):
            if f[i-1] == 0 and f[i] == 0 and f[i+1] ==0:
                f[i] = 1
                c += 1
        return c >= n
